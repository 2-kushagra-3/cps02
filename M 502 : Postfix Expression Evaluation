#include <stdio.h>
#include <string.h>
#include <ctype.h>
#include <stdlib.h>

int main(){
 char str[100];
    int c;
 scanf("%s",str);
 int stack[100],top=-1,i=0;
 while(str[i]){
   if(isalpha(str[i])){
     scanf("%d ",&stack[++top]);
    }
   else{
     int b=stack[top--];
     int a=stack[top--];
     if(str[i]=='+')c=a+b;
     else if(str[i]=='-')c=a-b;
     else if(str[i]=='*')c=a*b;
     else if(str[i]=='/')c=a/b;
     stack[++top]=c;
    }
    i++;
   }
    printf("%d ",stack[top]);
   return 0;
}
